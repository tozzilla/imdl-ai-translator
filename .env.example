# Configurazione environment per Translate IDML
# Copia questo file in .env e inserisci i tuoi valori

# OpenAI API Key (richiesta)
OPENAI_API_KEY=your_openai_api_key_here

# Modello OpenAI predefinito (opzionale)
OPENAI_MODEL=gpt-3.5-turbo

# Livello di logging (opzionale)
LOG_LEVEL=INFO

# Directory temporanea (opzionale)
TEMP_DIR=./temp

# Backup automatico file originali (opzionale)
AUTO_BACKUP=true